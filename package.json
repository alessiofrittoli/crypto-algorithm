{
	"name": "@alessiofrittoli/crypto-algorithm",
	"version": "0.1.0-alpha.1",
	"description": "Lightweight TypeScript library with Node.js Crypto Algortihm utilities",
	"author": {
		"name": "Alessio Frittoli",
		"email": "info@alessiofrittoli.it",
		"url": "https://alessiofrittoli.it"
	},
	"license": "MIT",
	"keywords": [
		"crypto",
		"algorithms"
	],
	"homepage": "https://github.com/alessiofrittoli/crypto-algorithm#readme",
	"bugs": {
		"url": "https://github.com/alessiofrittoli/crypto-algorithm/issues",
		"email": "info@alessiofrittoli.it"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alessiofrittoli/crypto-algorithm.git"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"typs": "./dist/index.d.ts"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"import": "./dist/types.mjs",
			"require": "./dist/types.js"
		}
	},
	"sideEffects": false,
	"scripts": {
		"build": "eslint && jest --ci --verbose && tsup",
		"lint": "eslint",
		"test": "jest --watchAll --verbose",
		"test:ci": "jest --ci --verbose",
		"test:jest": "pnpm test jest.test.ts",
		"test:algorithm": "pnpm test algorithm.test.ts"
	},
	"devDependencies": {
		"@eslint/js": "^9.16.0",
		"@jest/globals": "^29.7.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.1",
		"dotenv": "^16.4.5",
		"eslint": "^9.16.0",
		"globals": "^15.12.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	}
}
